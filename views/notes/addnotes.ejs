<% layout('../layout') -%>
<div class="page text-center">
    <div class="title">Add Notes</div>
    <div class="content">
        <form class="form" action="/addnotes" enctype="multipart/form-data" name="new-note">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Course</label>
                <div class="col-lg-6">
                    <select name="course">
                        <option value="btech">B.tech</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Year</label>
                <div class="col-lg-6">
                    <select name="year">
                        <option value="1">First Year</option>
                        <option value="2">Second Year</option>
                        <option value="3">Third Year</option>
                        <option value="4">Final Year</option>
                    </select>
                </div>
            </div>
        </form>
    </div>
</div>